    "scripts": {
        "build": "npm run clean && npm run compile && npm run compress",
        "compile": "webpack --config ./webpack.config.prod.js --output-path ../server/.build/public",
        "compress": "gzip -k ../server/.build/public/*",
        "dev": "webpack-dev-server --open --config ./webpack.config.dev.js",
        "clean": "rm -rf ../server/.build/public",
        "analyze": "npm run analyze-clean && npm run analyze-compile && npm run analyze-view",
        "analyze-clean": "rm -rf .build && mkdir .build",
        "analyze-compile": "webpack --config ./webpack.config.prod.js --profile --json > .build/webpack-profile.json",
        "analyze-view": "webpack-bundle-analyzer .build/webpack-profile.json"
    }
}
